{
    "name": "hauerheinrich/hh-tt-address-places",
    "type": "typo3-cms-extension",
    "description": "Simply adds places / companies TCA for EXT:tt_address.",
    "authors": [
        {
            "name": "Christian Hackl",
            "email": "chackl@hauer-heinrich.de",
            "homepage": "http://www.hauer-heinrich.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Hauer-Heinrich/hh_tt_address_places/issues",
        "source": "https://github.com/Hauer-Heinrich/hh_tt_address_places",
        "docs": "https://github.com/Hauer-Heinrich/hh_tt_address_places/blob/main/README.md"
    },
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/hauerheinrich"
        }
    ],
    "version": "2.3.0",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS", "address", "places", "companies", "Extension"],
    "replace": {
        "hauer-heinrich/hh_tt_address_places": "self.version",
        "typo3-ter/hh-tt-address-places": "self.version"
    },
    "require": {
        "typo3/cms-core": "^11.5.0 || ^12.4.0",
        "friendsoftypo3/tt-address": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "HauerHeinrich\\HhTtAddressPlaces\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web",
            "extension-key": "hh_tt_address_places"
        }
    }
}
